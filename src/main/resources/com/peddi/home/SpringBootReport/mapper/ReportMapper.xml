<?xml version="1.0" encoding="UTF-8" ?>
 
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peddi.report.mapper.ReportMapper">
    <resultMap id="reportResultMap" type="com.peddi.home.domain.Report">
        <result property="timestamp" column="timestamp"/>
        <result property="count" column="count"/>
    </resultMap>
    <select id="getCount" resultMap="reportResultMap">
        select timestamp, count from table)
    </select>
</mapper>
	
